<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="wxrmq.domain.WxUser_Tag" table="wxuser_tag">
 		<composite-id>  
            <!-- key-property标签表示哪一些属性对应复合主键 -->  
            <key-property name="uin"></key-property>  
            <key-property name="type"></key-property>  
            <key-property name="label"></key-property>  
            <key-property name="subLabel"></key-property>  
        </composite-id>  
		<property name="count" />
	</class>
</hibernate-mapping> 